<%- include('../layouts/admin-header.ejs') %>

<div class="dash-content container add">
 <h2 class="page-title">Add a Price Filter</h2>
    <a href="/admin/price-filters" class="btn"><i class="fa fa-angle-left"></i></a>
   
    <br><br>
    <div class="row">
        <form id="add-edit" method="post" action="/admin/price-filters/add-price-filter">
            <% error.forEach(function(err){ %>
                <div class="alert alert-success" role="alert">
                    <%= err %>
                </div>
              <% }) %>
            <div class="form-group">
                <!-- <label for="title">Title</label> -->
                <input id="title" type="text" class="box" name="title" value="<%= title %>" placeholder="Title">
            </div>
            <br>
            <button type="submit" class="btn btn-default">Submit</button>
        </form>
    </div>
</div>
    
</section>

<%- include('../layouts/admin-footer.ejs') %>
